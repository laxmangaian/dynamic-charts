{
    "title": "Bar Line Chart",
    "width": 700,
    "height": 400,
    "margin": {
        "top": 40,
        "right": 40,
        "bottom": 50,
        "left": 60
    },
    "dataKeys": {
        "xKey": "x",
        "yKey": "y"
    },
    "xLabel": "Day",
    "yLabel": "Value",
    "line": {
        "stroke": "yellow",
        "strokeWidth": 3,
        "pointShow": true,
        "pointRadius": 2,
        "pointColor": "red"
    },
    "bar": {
        "fill": "red",
        "stroke": "#333333",
        "strokeWidth": 1,
        "opacity": 0.9,
        "cornerRadius": 3,
        "hoverFill": "#ff0000",
        "animationDuration": 800
    }
}